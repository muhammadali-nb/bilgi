{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "paths": {
      "@utils/*": [
        "./utils/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@composables/*": [
        "./composables/*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@language-constants": [
        "./locales/constants.ts"
      ]
    }
  }
}